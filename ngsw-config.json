{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.png",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/*.map"
        ]
      }
    },
    {
      "name": "required-assets",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/images/*.*",
          "/fonts/*.*",
          "/icons/*.*",
          "/audio/**/*.*"
        ]
      }
    },
    {
      "name": "lazy-assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [

        ]
      }
    }
  ]
}
